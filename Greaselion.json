[
  {
    "urls": [
      "https://github.com/*",
      "https://www.github.com/*",
      "https://gist.github.com/*"
    ],
    "scripts": [
      "scripts/brave_rewards/publisher/github/github.bundle.js"
    ],
    "preconditions": {
      "github-tips-enabled": true
    },
    "messages": "scripts/brave_rewards/publisher/github/_locales"
  },
  {
    "urls": [
      "https://twitter.com/*",
      "https://*.twitter.com/*"
    ],
    "scripts": [
      "scripts/brave_rewards/publisher/twitter/twitter.bundle.js"
    ],
    "preconditions": {
      "twitter-tips-enabled": true
    },
    "messages": "scripts/brave_rewards/publisher/twitter/_locales"
  },
  {
    "urls": [
      "https://m.youtube.com/*",
      "https://www.youtube.com/*"
    ],
    "scripts": [
      "scripts/brave_rewards/publisher/youtube/youtube.bundle.js"
    ],
    "preconditions": {
      "rewards-enabled": true
    }
  }
]
